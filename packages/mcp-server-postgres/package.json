{
  "name": "@supabase/mcp-server-postgres",
  "version": "0.1.0",
  "description": "MCP server for querying Postgres and retrieving metadata",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsup --clean",
    "prepublishOnly": "npm run build",
    "test": "vitest"
  },
  "files": [
    "dist/**/*"
  ],
  "bin": {
    "mcp-server-postgres-meta": "./dist/stdio.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.cjs"
    },
    "./queriers/postgres": {
      "import": "./dist/queriers/postgres/index.js",
      "types": "./dist/queriers/postgres/index.d.ts",
      "default": "./dist/queriers/postgres/index.cjs"
    },
    "./queriers/management-api": {
      "import": "./dist/queriers/management-api/index.js",
      "types": "./dist/queriers/management-api/index.d.ts",
      "default": "./dist/queriers/management-api/index.cjs"
    },
    "./queriers/pglite": {
      "import": "./dist/queriers/pglite/index.js",
      "types": "./dist/queriers/pglite/index.d.ts",
      "default": "./dist/queriers/pglite/index.cjs"
    }
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.2.15",
    "@gregnr/postgres-meta": "^0.82.0-dev.2",
    "@modelcontextprotocol/sdk": "^1.4.1",
    "@supabase/mcp-utils": "0.1.1",
    "openapi-fetch": "^0.13.4",
    "postgres": "^3.4.5",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.1"
  },
  "devDependencies": {
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/node": "^22.8.6",
    "openapi-typescript": "^7.5.0",
    "prettier": "^3.3.3",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  }
}
